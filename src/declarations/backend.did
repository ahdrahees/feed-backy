type UserFeedbacksResult = 
 variant {
   err: Error;
   ok: vec Feedback;
 };
type Result_1 = 
 variant {
   err: text;
   ok: nat;
 };
type Result = 
 variant {
   err: text;
   ok: BrandBasicInfo;
 };
type Registration = 
 variant {
   Brand: BrandRegisterArg;
   User: text;
 };
type RegisterResult = 
 variant {
   err: text;
   ok;
 };
type QueryUserResult = 
 variant {
   err: Error;
   ok: QueryUser;
 };
type QueryUser = 
 record {
   balance: nat;
   id: nat;
   lastFeedback: int;
   name: text;
   "principal": text;
   totalFeedback: nat;
 };
type QueryPostResult = 
 variant {
   err: Error;
   ok: QueryPost;
 };
type QueryPost = 
 record {
   brandName: text;
   created: int;
   filledspot: nat;
   owner: text;
   postId: nat;
   question: vec text;
