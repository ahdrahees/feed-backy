  getAllFeedbackAndPost: () -> (AllFeedbackAndPostResult) query;
  getAllOpenPosts: () -> (AllOpenPostsResult) query;
  getAllPostAndFeedbacks: () -> (AllPostAndFeedbacksResult) query;
